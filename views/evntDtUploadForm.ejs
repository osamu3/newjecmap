            //アップロードイベントの前処理
            function submitFormFn() {
				let eventCategory = $(form1)[0].HeadrCategory.value;//イベントの種別（事故、落下物、死骸処理。。。）
                if ( eventCategory == "") {
                    alert("事象種別が入力されていません。");
                    return false;//呼び出し側へ戻ってから処理を中止(post注止)
                }
                if ($(form1)[0].HeadrTarget.value == "") {
                    alert("対象案件が入力されていません。");
                    return false;//呼び出し側へ戻ってから処理を中止(post注止)
                }
                if ($(form1)[0].HeadrAttender.value == "") {
                    alert("処理対応者が入力されていません。");
                    return false;//呼び出し側へ戻ってから処理を中止(post注止)
                }
                if ($(form1)[0].HeadrInformer.value == "") {
                    alert("通報者が入力されていません。");
                    return false;//呼び出し側へ戻ってから処理を中止(post注止)
                }
                /////////////////////////////////////////
                //その他、住所、キロポス、概要、内容、発生時分などをチェックする
				////////////////////////////////////////

				//親画面の事象リストに当該事象を追加
                //参考：親画面（子画面の呼び出し元）にデータを渡す方法 JQueryの使い方に若干のKwhwあり、window.opener.$(…

				//送信データをユニーク化するため、現在の時分秒ミリ秒にして、ポストする。
				let eventUId = Date.now();
				let linkStr = '['+eventCategory+']'+$("#id_HeadrYear").val()+'/'+$("#id_HeadrMonth").val()+'/'+$("#id_HeadrDay").val();
				linkStr +=' '+$('#id_HeadrHour').val()+':'+$('#id_HeadrMinute').val()+' '+$('#id_HeadrCity').val()+$('#id_HeadrAddres').val();

				//親画面（この画面を開いた画面=window.opener）
				//①全ての事象に登録
				window.opener.$('#ulListEvnt-0').append('<a href="#"style=" list-style: none; margin-left: 0; padding-left: 0em; text-indent: -2.5em; font-size: 10.5pt"><li class="evntList" id="' + eventUId + '" onClick=eventListClick(this)>' + linkStr+ '</li></a>')
				switch( eventCategory ) {
					//②カテゴリー落下物に登録
					case '落下物':
						window.opener.$('#ulListEvnt-1').append('<a href="#"   style=" list-style: none; margin-left: 0; padding-left: 0em; text-indent: -2.5em; font-size: 10.5pt"><li class="evntList" id="' + eventUId + '" onClick=eventListClick(this)>' + linkStr + '</li></a>');
						break;
					//③カテゴリー事故に登録
					case '事故':
						window.opener.$('#ulListEvnt-2').append('<a href="#"   style=" list-style: none; margin-left: 0; padding-left: 0em; text-indent: -2.5em; font-size: 10.5pt"><li class="evntList" id="' + eventUId + '" onClick=eventListClick(this)>' + linkStr + '</li></a>');
					//④カテゴリー災害に登録
					case '災害':
						window.opener.$('#ulListEvnt-3').append('<a href="#"   style=" list-style: none; margin-left: 0; padding-left: 0em; text-indent: -2.5em; font-size: 10.5pt"><li class="evntList" id="' + eventUId + '" onClick=eventListClick(this)>' + linkStr + '</li></a>');
						break;
